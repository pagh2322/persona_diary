<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/fontBColor" />
            <corners android:topLeftRadius="10dp" android:bottomLeftRadius="10dp"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/bgColor" />
            <stroke android:width="1dp" android:color="@color/disableColor" />
            <corners android:topLeftRadius="10dp" android:bottomLeftRadius="10dp"/>
        </shape>
    </item>
</selector>